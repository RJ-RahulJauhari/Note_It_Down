<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <stroke android:color="@color/IconColor" android:width="@dimen/_2mdp"></stroke>
            <corners android:radius="@dimen/_30mdp"></corners>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/IconColor"></solid>
            <corners android:radius="@dimen/_30mdp"></corners>
        </shape>
    </item>

</selector>